<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="Selection">
        <enum name="LEFT" value="0" />
        <enum name="RIGHT" value="1" />
    </attr>


    <attr name="text1" format="string" />
    <attr name="text2" format="string" />
    <attr name="selectedIndex" format="integer" />

    <declare-styleable name="ToogleSwitch">

        <attr name="Selection"/>
        <attr name="text1" />
        <attr name="text2" />
    </declare-styleable>

    <declare-styleable name="vAnimLayout">
        <attr name="selectedIndex"/>
    </declare-styleable>
</resources>